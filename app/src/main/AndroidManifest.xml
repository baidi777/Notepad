<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.cheat.notepad"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".atys.AtyLogin"/>
        <activity android:name=".atys.AtyRegister"/>
        <activity android:name=".atys.AtyMain"/>
        <activity android:name=".atys.AtyPasswd"/>
        <activity android:name=".atys.AtyPassword"/>
        <activity android:name=".atys.AtyAddContent"/>
    </application>

</manifest>
